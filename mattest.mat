pinmaps
{
    cs:DIO2,
    clk:DIO1,
    enable:DIO5
}
pingroups
{
    pgroup0 : DIO11,DIO22,DIO33,DIO44;
    pgroup1 : DIO85,DIO94,DIO84,DIO83;
    pgroup2 : DIO55,DIO66,DIO77,DIO88
}

timeset
{
    tset0 : 10.555 s;
    tset1 : 0.00000002 s
}

voltagelevels
{
    cs : vil=0.0,vih=3.3,vol=1.6,voh=1.7,vtermm=2.0;
    pgroup0 : vil=0.0,vih=3.3,vol=1.6,voh=1.7,vterm=2.0
}
  

/* 
multi-line comment -> skipped
*/

//single line comment -> skipped

def functionwhatever123 ()
{ 
    timeset tset0 ;
    set cs=0,clk=1,pgroup0=0010;
    read cs,clk;
    
    if cs=1,clk=0
    {
        set cs=1,clk=0;
        if cs=1,clk=0
        { 
            read cs,clk;
        }  
    }
    else{
        set enable=1;
    }
  
    if cs=1,clk=0{ 
        read cs,clk;
    } 

    goto label_example;


    inc pgroup0,1:0110;
label_example:  
    dec pgroup1,1100:0010;

}

def main ()
{
    timeset tset1 ;
    
    repeat 16 : set cs=1,clk=0;
    //set cs=1,clk=0;
    functionwhatever123();

    loop 1125
    {
        set cs=1,clk=0;
        set cs=0,clk=1;

        loop 11
        {
            set enable=1,cs=0;
        }
    }

    while cs=1,clk=1
    {
       set cs=0,clk=0,pgroup0=0000;
    }
}
